{
  "extends": "airbnb-base",

  "rules": {
    "quotes": "off",
    "linebreak-style": "off",
    "spaced-comment": "off",
    "import/no-anonymous-default-export": "off",
    "no-console": "off",
    "consistent-return": "off",
    "no-shadow": "off",
    "comma-dangle": "off",
    "no-trailing-spaces": "warn",
    "no-param-reassign": "off",
    "no-labels": "off",
    "no-restricted-syntax": "off",
    "no-lone-blocks": "off",
    "no-unused-labels": "warn"
  }
}
